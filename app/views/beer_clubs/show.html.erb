<p id="notice"><%= notice %></p>

<h2>
  <%= @beer_club.name %> <%= @beer_club.city %>
</h2>

<p>
  Founded
  <%= @beer_club.founded %>
</p>

<strong>Members</strong>
<ul>
  <% @beer_club.users.each do |user| %>
    <li><%= link_to user.username, user_path(user)%></li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_beer_club_path(@beer_club) %>
